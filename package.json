{
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"babel -w ./src --out-dir dist --extensions .ts,.tsx\"",
    "build": "next build",
    "start": "next start -p 4000",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:build": "babel -w ./src --out-dir dist --extensions .ts,.tsx",
    "m:create": "typeorm migration:create",
    "m:run": "typeorm migration:run -d dist/data-source.js",
    "m:revert": "typeorm migration:revert -d dist/data-source.js",
    "e:create": "typeorm entity:create"
  },
  "dependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.20.5",
    "@babel/runtime": "7.20.6",
    "@types/marked": "^4.0.8",
    "axios": "1.2.1",
    "classnames": "^2.3.2",
    "github-markdown-css": "^5.1.0",
    "iron-session": "6.3.1",
    "lodash": "4.17.21",
    "marked": "^4.2.5",
    "md5": "2.3.0",
    "next": "latest",
    "pg": "8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "0.1.13",
    "sass": "1.56.1",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.20.5",
    "@types/axios": "0.14.0",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "4.14.191",
    "@types/md5": "2.3.2",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "typescript": "4.9.3"
  }
}
