    "m:revert": "typeorm migration:revert -d dist/data-source.js"
  },
  "dependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "^7.20.5",
    "@babel/runtime": "^7.20.6",
    "next": "latest",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.56.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "typescript": "^4.9.3"
  }
}
